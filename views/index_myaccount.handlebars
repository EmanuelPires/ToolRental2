<div class="container emp-profile" id="cont">
    <form method="post">
        <div class="row">
            <div class="col-md-3">
                <div class="profile-img">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS-Vq3P0m0WI77jVgVXEJUczsun5CddxdtdCL5bE70gYUEZG2LVZQ"
                        class="img-raised rounded img-fluid" alt="" />
                    <br>
                    <br>
                    <hr>
                    <h5>
                        {{profile.dataValues.Name}}
                    </h5>
                    <h6>
                        Toolit Premium Renter
                    </h6>


                </div>

            </div>
            <div class="col-md-1">






            </div>
            <div class="col-md-8">
                <div class="section section-tabs">
                    <div class="container">
                        <!--                nav tabs	             -->
                        <div id="nav-tabs">
                            <h3>User Profile</h3>
                            <div class="row">
                                <div class="col-md-12">

                                    <!-- Tabs with icons on Card -->
                                    <!-- <div class="card card-nav-tabs"> -->
                                    <div class="card-header card-header-primary">
                                        <!-- colors: "header-primary", "header-info", "header-success", "header-warning", "header-danger" -->
                                        <div class="nav-tabs-navigation">
                                            <div class="nav-tabs-wrapper">
                                                <ul class="nav nav-tabs" data-tabs="tabs">
                                                    <li class="nav-item">
                                                        <a class="nav-link active" href="#profile" data-toggle="tab">
                                                            <i class="material-icons">face</i> User Info
                                                        </a>
                                                    </li>

                                                    <li class="nav-item">
                                                        <a class="nav-link" href="#orders" data-toggle="tab">
                                                            <i class="material-icons">history</i> Order History
                                                        </a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" href="#products" data-toggle="tab">
                                                            <i class="material-icons">share</i> Products Offered
                                                        </a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" href="#addProduct" data-toggle="tab">
                                                            <i class="material-icons">add</i> Add Product
                                                        </a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a class="nav-link" href="#settings" data-toggle="tab">
                                                            <i class="material-icons">build</i> Settings
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    {{>myAccount/userInfo}}
                                    <div class="tab-pane" id="orders">
                                        <table id="dtDynamicVerticalScrollExample" class="table table-striped table-bordered table-sm"
                                            cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th class="th-sm">Order Number
                                                    </th>
                                                    <th class="th-sm">Order Date
                                                    </th>
                                                    <th class="th-sm">Quantity Ordered
                                                    </th>


                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{#each Order}}

                                                {{>myAccount/orders}}

                                                {{/each}}
                                            </tbody>

                                        </table>

                                    </div>
                                    <div class="tab-pane" id="products">
                                        <table id="dtDynamicVerticalScrollExample" class="table table-striped table-bordered table-sm"
                                            cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th class="th-sm">Product Name
                                                    </th>
                                                    <th class="th-sm">Unit Price
                                                    </th>
                                                    <th class="th-sm">Units In Stock
                                                    </th>
                                                    <th class="th-sm">Units On Order
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{#each Product}}
                                                {{>myAccount/myProducts}}
                                                {{/each}}
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="tab-pane" id="addProduct">
                                        <div class="form-group row">
                                            <label class="col-lg-3 col-form-label form-control-label">Product
                                                Name</label>
                                            <div class="col-lg-9">
                                                <input class="form-control" type="text" value="" id="productName" />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-3 col-form-label form-control-label">Unit
                                                Price</label>
                                            <div class="col-lg-9">
                                                <input class="form-control" type="number" value="" id="unitPrice" />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-3 col-form-label form-control-label">Unit
                                                Quanitity</label>
                                            <div class="col-lg-9">
                                                <input class="form-control" type="number" value="" id="unitStock" />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-lg-3 col-form-label form-control-label">Image URL
                                                Name</label>
                                            <div class="col-lg-9">
                                                <input class="form-control" type="text" value="" id="imageURL" />
                                            </div>
                                        </div>
                                        <button id="submitProduct" data-id="{{profile.CustomerID}}">Submit</button>
