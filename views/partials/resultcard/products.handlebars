<div class="col-3">
    <div class="card-fluid shadow p-3">
        <img src="{{dataValues.Image}}" width="400px" height="250px" alt="">
        <h3 class="name">{{dataValues.Product_Name}}</h3>
        <hr />
        <ul>

            <li>UnitPrice: {{dataValues.UnitPrice}}</li>
            <li>Units Available: {{dataValues.UnitsInStock}}</li>
            <li>Renter UserName: {{dataValues.Customer.dataValues.Name}}</li>
        </ul>
        <button data-id={{dataValues.ProductID}} data-price={{dataValues.UnitPrice}}
            data-user={{dataValues.Customer.dataValues.CustomerID}} data-stock={{dataValues.UnitsInStock}}
            data-order={{dataValues.UnitsOnOrder}} class="orderBtn">ORDER</button>

    </div>
</div>